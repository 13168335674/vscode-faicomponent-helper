{
  "name": "vscode-faicomponent-helper",
  "displayName": "faicomponent helper",
  "description": "A vscode extension for faicomponent",
  "version": "0.1.0",
  "publisher": "adi",
  "repository": {
    "type": "git",
    "url": "https://github.com/13168335674/vscode-faicomponent-helper"
  },
  "engines": {
    "vscode": "^1.30.0"
  },
  "icon": "faicomponent.png",
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:vue",
    "onCommand:faicomponent-helper.search"
  ],
  "contributes": {
    "commands": [
      {
        "command": "faicomponent-helper.search",
        "title": "faicomponent-helper.search"
      }
    ],
    "keybindings": [
      {
        "command": "faicomponent-helper.search",
        "key": "shift+cmd+i",
        "when": "editorTextFocus"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/faicomponent.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Faicomponent Helper Configuration",
      "properties": {
        "faicomponent-helper.indent-size": {
          "type": "number",
          "default": 2,
          "description": "Indentation size of snippets"
        },
        "faicomponent-helper.quotes": {
          "type": "string",
          "default": "double"
        },
        "faicomponent-helper.link-url": {
          "type": "string",
          "default": "http://fa.docs.aaa.cn"
        }
      }
    }
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "pretty": "^2.0.0",
    "shelljs": "^0.8.3"
  }
}
